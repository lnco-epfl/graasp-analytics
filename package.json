{
  "name": "graasp-research",
  "version": "0.1.8",
  "private": true,
  "author": "React EPFL",
  "contributors": [
    "Hagop Taminian",
    "Juan Carlos Farah"
  ],
  "dependencies": {
    "@material-ui/core": "4.9.13",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.52",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "google-map-react": "1.1.7",
    "i18next": "19.4.4",
    "lodash": "4.17.20",
    "prop-types": "15.7.2",
    "qs": "6.9.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-ga": "2.7.0",
    "react-i18next": "11.4.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1",
    "react-select": "3.1.0",
    "recharts": "1.8.5",
    "supercluster": "7.0.0",
    "use-supercluster": "0.2.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:local": "PORT=80 react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f ./.env.dev yarn build",
    "build:prod": "env-cmd -f ./.env.prod yarn build",
    "test": "react-scripts test",
    "test:once": "cross-env CI=true env-cmd -f ./.env.test react-scripts test",
    "eject": "react-scripts eject",
    "hooks:uninstall": "node node_modules/husky/husky.js uninstall",
    "hooks:install": "node node_modules/husky/husky.js install",
    "lint": "eslint .",
    "prettier:check": "prettier --check 'src/**/*.js'",
    "prettier:write": "prettier --write 'src/**/*.js'",
    "release": "git fetch --tags && git add CHANGELOG.md && standard-version -a",
    "release:first": "git fetch --tags && git add CHANGELOG.md && standard-version -a --first-release"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "cross-env": "7.0.2",
    "env-cmd": "10.1.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "husky": "4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "standard-version": "8.0.2",
    "typescript": "3.8.3"
  }
}
