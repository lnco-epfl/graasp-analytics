{
  "name": "graasp-analytics",
  "version": "0.1.12",
  "private": true,
  "author": "Graasp Association",
  "contributors": [
    "Hagop Taminian",
    "Juan Carlos Farah",
    "Kim Lan Phan Hoang"
  ],
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@graasp/query-client": "github:graasp/graasp-query-client",
    "@graasp/sdk": "github:graasp/graasp-sdk",
    "@graasp/ui": "github:graasp/graasp-ui#v0.3.0",
    "@mui/icons-material": "5.10.9",
    "@mui/lab": "5.0.0-alpha.107",
    "@mui/material": "5.10.13",
    "@sentry/react": "7.19.0",
    "@sentry/tracing": "7.19.0",
    "google-map-react": "2.1.10",
    "i18next": "21.8.1",
    "immutable": "4.0.0",
    "lodash.capitalize": "4.2.1",
    "lodash.sortby": "4.7.0",
    "lodash.truncate": "4.4.2",
    "prop-types": "15.8.1",
    "qs": "6.10.3",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-ga": "3.3.0",
    "react-i18next": "11.16.9",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "react-select": "5.3.2",
    "react-toastify": "9.0.1",
    "recharts": "2.1.9",
    "supercluster": "7.1.5",
    "use-supercluster": "0.4.0"
  },
  "scripts": {
    "pre-commit": "yarn prettier:check && yarn lint",
    "start": "env-cmd -f ./.env.local react-app-rewired start",
    "start:test": "env-cmd -f ./.env.test react-app-rewired start",
    "start:ci": "react-app-rewired -r @cypress/instrument-cra start",
    "build": "react-app-rewired build",
    "dist": "env-cmd -f ./.env.production react-app-rewired build",
    "eject": "react-scripts eject",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "prettier:check": "prettier --check 'src/**/*.js'",
    "prettier:write": "prettier --write 'src/**/*.js'",
    "release": "standard-version -a",
    "release:first": "standard-version -a --first-release",
    "release:minor": "standard-version -a --release-as minor",
    "release:major": "standard-version -a --release-as major",
    "release:patch": "standard-version -a --release-as patch",
    "postinstall": "husky install",
    "cypress:open": "env-cmd -f ./.env.test cypress open",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser chrome --spec \"cypress/e2e/*.cy.ts\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@babel/eslint-plugin": "7.17.7",
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@cypress/code-coverage": "3.10.0",
    "@cypress/instrument-cra": "1.4.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/jest": "29.0.3",
    "@types/node": "18.7.21",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "cross-env": "7.0.3",
    "cypress": "11.0.1",
    "env-cmd": "10.1.0",
    "eslint": "8.15.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "husky": "8.0.1",
    "istanbul-lib-coverage": "3.2.0",
    "nyc": "15.1.0",
    "prettier": "2.7.1",
    "react-app-rewired": "2.2.1",
    "standard-version": "9.3.2",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "@graasp/sdk": "github:graasp/graasp-sdk"
  },
  "packageManager": "yarn@3.2.0"
}
